---
import {ViewTransitions} from 'astro:transitions'
import { Picture } from 'astro:assets'

import Layout from '../layouts/Layout.astro'

interface Props {
    title: string
}

const { title } = Astro.props
---
<Layout title={title}>
    <h1 class="text-5xl text-primary text-center mb-12">{title}</h1>
    <slot />
</Layout>
